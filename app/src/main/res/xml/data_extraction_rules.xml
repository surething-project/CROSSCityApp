<?xml version="1.0" encoding="utf-8"?>
<!--
    Shared preferences backups cannot be allowed because after uninstalling the application,
    the backup encryption file cannot be decrypted after a fresh install.
-->
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="sharedpref" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" />
    </device-transfer>
</data-extraction-rules>
